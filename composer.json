{
    "name": "mwi/laravel-kit",
    "description": "MWI Starter Kit for Laravel Projects",
    "keywords": ["starter", "laravel"],
    "license": "MIT",
    "type": "library",
    "require": {
        "laravelcollective/html": "^5.5",
        "doctrine/dbal": "^2.6",
        "bugsnag/bugsnag-laravel": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "MWI\\LaravelKit\\ScriptHandler::postInstall"
        ]
    },
    "autoload": {
        "psr-4": {
            "MWI\\LaravelKit\\": ""
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
